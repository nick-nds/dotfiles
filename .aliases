#vim to nvim
alias vim='nvim'
alias nv='nvim'


#ls alias
alias ll='ls --color=tty -l'

alias recursiveextact='find . -name "*.zip" | while read filename; do unzip -o -d "`dirname "$filename"`" "$filename"; done;'


